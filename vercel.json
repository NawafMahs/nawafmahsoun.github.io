{
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  },
  "buildCommand": "STATIC_BUILD=true npm run build",
  "outputDirectory": "build/client",
  "installCommand": "npm ci",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}